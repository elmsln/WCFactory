{
  "name": "Twig component",
  "description": "Process twig on the fly",
  "wcfactory": {
    "customElementClass": "HTMLElement",
    "templateReturnFunctionPart": "getTemplate() {\n    return ",
    "propertyBinding": {
      "prefix": "{{ ",
      "suffix": " }}"
    }
  },
  "main": "TwigComponent/TwigComponent.js",
  "version": "0.0.1",
  "scripts": {
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "dependencies": {
    "twig": "^1.12.0"
  },
  "devDependencies": {
    "web-animations-js": "2.3.2",
    "@lrnwebcomponents/deduping-fix": "2.1.1",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@webcomponents/webcomponentsjs": "2.4.0",
    "concurrently": "4.1.2",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "lodash": "4.17.15",
    "polymer-cli": "1.9.11",
    "polymer-build": "3.1.4",
    "wct-browser-legacy": "1.0.2"
  }
}
